@model Instructor
@{
    ViewData["Title"] = "Details";
}

<h1>@Model.Name Details</h1>

<div class="card" >
    <ul class="list-group list-group-flush">
        <li class="list-group-item">ID: @Model.Id</li>
        <li class="list-group-item">Name: @Model.Name</li>
        <li class="list-group-item">Salary: @Model.Salary</li>
        <li class="list-group-item">Address @Model.Address</li>
        <li class="list-group-item">Department Id: @Model.DeptId</li>
        <li class="list-group-item">Course Id: @Model.CourseId</li>
    </ul>
</div>

