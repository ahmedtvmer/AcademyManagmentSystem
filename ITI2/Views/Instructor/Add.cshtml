@model Instructor
@{
    ViewData["Title"] = "Add";
}

<h1>Add</h1>

<form asp-action="Add" asp-controller="Instructor" method="post">

    <label asp-for="Name">Name</label>
    <input class="form form-control m-2" asp-for="Name" />

    <label asp-for="Salary">Salary</label>
    <input class="form form-control m-2" asp-for="Salary" />

    <label asp-for="Address">Address</label>
    <input class="form form-control m-2" asp-for="Address" />

    <label asp-for="DeptId">Department ID</label>
    <input class="form form-control m-2" asp-for="DeptId" />

    <label asp-for="CourseId">Course ID</label>
    <input class="form form-control m-2" asp-for="CourseId" />

    <button type="submit" class="btn btn-primary">Save</button>
</form>

